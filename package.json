{
  "name": "react_typescript_project_template",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "server:watch_babel": "babel src --config-file ./config/server/babel.config.js --out-dir dist --extensions \".ts,.tsx\"  -w",
    "server:start": "cross-env NODE_ENV='development' nodemon --trace-warnings dist/server/index.js run",
    "client:dev_server": "cross-env NODE_ENV='development' NODE_PATH='./src' webpack serve --config config/webpack.config.js --content-base dist/client --open",
    "client:build": "cross-env NODE_ENV='production' webpack  --config config/webpack.config.js --color",
    "build": "babel src --config-file ./config/server/babel.config.js --out-dir dist --extensions \".ts,.tsx\" && npm run client:build"
  },
